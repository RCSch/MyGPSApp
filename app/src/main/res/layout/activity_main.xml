<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/label_latitude"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/latitude"
        android:layout_margin="32dp"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_latitude"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="32dp"
        android:text="lat"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@+id/label_latitude"
        app:layout_constraintTop_toTopOf="parent"
        />

    <TextView
        android:id="@+id/label_longitude"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/longitude"
        android:layout_margin="32dp"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/label_latitude" />

    <TextView
        android:id="@+id/tv_longitude"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="32dp"
        android:text="long"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@+id/label_longitude"
        app:layout_constraintTop_toBottomOf="@+id/tv_latitude"
        />

    <TextView
        android:id="@+id/label_altitude"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/altitude"
        android:layout_margin="32dp"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/label_longitude" />

    <TextView
        android:id="@+id/tv_altitude"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="32dp"
        android:text="aaaltitude"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@+id/label_altitude"
        app:layout_constraintTop_toBottomOf="@+id/tv_longitude"
        />

    <TextView
        android:id="@+id/label_speed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/speed"
        android:layout_margin="32dp"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/label_altitude" />

    <TextView
        android:id="@+id/tv_speed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="32dp"
        android:text="Hastighed"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@+id/label_speed"
        app:layout_constraintTop_toBottomOf="@+id/tv_altitude"
        />

    <TextView
        android:id="@+id/label_bearing"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/bearing"
        android:layout_margin="32dp"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/label_speed" />

    <TextView
        android:id="@+id/tv_bearing"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="32dp"
        android:text="bearings"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@+id/label_bearing"
        app:layout_constraintTop_toBottomOf="@+id/tv_speed"
        />
    <TextView
        android:id="@+id/label_accuracy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/accuracy"
        android:layout_margin="32dp"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/label_bearing" />

    <TextView
        android:id="@+id/tv_accuracy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="32dp"
        android:text="accuracy"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@+id/label_accuracy"
        app:layout_constraintTop_toBottomOf="@+id/tv_bearing"
        />

    <Button
        android:id="@+id/btn_update"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Update!"
        app:layout_constraintEnd_toEndOf="@id/label_accuracy"
        app:layout_constraintTop_toBottomOf="@+id/label_accuracy"
       />


</androidx.constraintlayout.widget.ConstraintLayout>